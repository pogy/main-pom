<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.opentae.data.mall.interfaces.ShiguStoreNumMapper" >

  <!-- 更新档口号信息 -->
  <update id="updateStoreNumByStoreId">
  	UPDATE shigu_store_num
  		SET
	  	  store_num = #{storeNum}
	WHERE
		store_id = #{storeId}
  </update>
</mapper>