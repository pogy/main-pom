<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


    <!--ES的访问配制-->
    <bean id="esCofing" class="tinytoes.ESConfig">
        <property name="esUrl" value="${ES_URL}"/>
        <property name="esPort" value="${ES_PORT}"/>
        <property name="clusterName" value="${ES_CLUSTER_NAME}"/>
        <property name="applic" value="${ES_APPLIC}"/>
        <property name="goodsKU" value="${ES_GOODS_KU}"/>
    </bean>


</beans>