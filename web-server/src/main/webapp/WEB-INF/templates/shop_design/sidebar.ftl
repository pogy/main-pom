        <li class="page-head-slide slide" data-slide="page-head">
            <b class="close-icon J_ToolbarItemClose"></b>
            <div class="J_PageHeadBg page-head-bg-content">
                <div class="page-head-bg-content-wrap J_PageHeadBgWrap" style="height: 499px;">
                <#assign showImg = container.banner.allModules[0].moduleSet.bannerOption.imgShow == 1>
                <#assign spacing = container.banner.allModules[0].moduleSet.bannerOption.spacing == 1>
                    <div class="page-head-bp">
                        <span>是否显示店铺招牌：</span>
                        <div class="operate-btn" style="float: right; margin-right: 10px;">
                            <span class="open ${showImg?string("onselected","")}" data-show-img="" data-value="1">显示</span>
                            <span class="close ${showImg?string("","onselected")}" data-show-img="" data-value="0">隐藏</span>
                        </div>
                    </div>
                    <div class="page-head-bp">
                        <span>页头下边距10像素：</span>
                        <div class="operate-btn" style="float: right; margin-right: 10px;">
                            <span class="J_TenPx open ${spacing?string("onselected","")}" data-show-spacing="" data-value="1">显示</span>
                            <span class="J_TenPx close ${spacing?string("","onselected")}" data-show-spacing="" data-value="0">隐藏</span>
                        </div>
                    </div>
                    <div style="margin-top: 20px;">
                        <a href="javascript:;" data-hd-apply="">应用当前设置</a>
                        <span style="color: #ccc;">&nbsp;*将会在所有页面生效</span>
                    </div>
                </div>
            </div>
